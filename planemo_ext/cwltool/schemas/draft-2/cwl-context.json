    },
    "fields": {
        "@id": "avro:fields",
        "@container": "@list"
    },
    "symbols": {
        "@id": "avro:symbols",
        "@container": "@list"
    },
    "items": {
        "@id": "avro:items",
        "@type": "@vocab"
    },
    "values": {
        "@id": "avro:values",
        "@type": "@vocab"
    },
    "binding": "cwl:binding",

    "requirementType": "@type",

    "CommandLineTool": "cwl:CommandLineTool",
    "baseCommand": {
        "@id": "cwl:baseCommand",
        "@container": "@list"
    },
    "arguments": {
        "@id": "cwl:arguments",
        "@container": "@list"
    },
    "stdin": "cwl:stdin",
    "stdout": "cwl:stdout",
    "inputs": "wfdesc:hasInput",
    "outputs": "wfdesc:hasOutput",
    "requirements": "cwl:requirements",
    "hints": "cwl:hints",
    "schemaDefs": "cwl:schemaDefs",
    "expressionDefs": "cwl:expressionDefs",
    "fileDefs": "cwl:fileDefs",

    "filename": "cwl:filename",
    "javascript": "cwl:JavascriptExpression",

    "DockerImage": "cwl:DockerImage",
    "dockerPull": "cwl:dockerPull",
    "dockerImport": "cwl:dockerImport",
    "dockerImageId": "cwl:dockerImageId",

    "CPURequirement": "cwl:CPURequirement",
    "preferredCPUCores": "cwl:preferredCPUCores",

    "MemoryRequirement": "cwl:MemoryRequirement",
    "memoryInMiB": "cwl:memoryInMiB",


    "Workflow": "cwl:Workflow",
    "dataLinks": "wfdesc:hasDataLink",
    "source": "wfdesc:hasSource",
    "destination": "wfdesc:hasSink",
    "steps": "wfdesc:hasSubProcess",

    "app": {
        "@id": "cwl:hasImplementation",
        "@type": "@id"
    },
    "impl": {
        "@id": "cwl:linksToPort",
        "@type": "@id"
    }

}
