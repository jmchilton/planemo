{
    "cwl": "http://github.com/common-workflow-language#",
    "avro": "http://github.com/common-workflow-language#avro/",
    "wfdesc": "http://purl.org/wf4ever/wfdesc#",
    "cnt": "http://www.w3.org/2011/content#",
    "sch": "http://schema.org/",
    "dct": "http://purl.org/dc/terms/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",

    "value": "rdf:value",

    "null": "avro:null",
    "boolean": "avro:boolean",
    "int": "avro:int",
    "long": "avro:long",
    "float": "avro:float",
    "double": "avro:double",
    "bytes": "avro:bytes",
    "string": "avro:string",
    "record": "avro:record",
    "enum": "avro:enum",
    "array": "avro:array",
    "map": "avro:map",
    "File": "cwl:File",

    "Expression": "cwl:Expression",
    "expressionType": "@type",
    "ref": "@id",

    "position": "cwl:position",
    "prefix": "cwl:prefix",
    "separator": "cwl:separator",
